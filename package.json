{
   "name": "tenant_manager",
   "version": "1.1.7",
   "description": "(AppBuilder) A service to manage the site&#39;s tenants",
   "main": "app.js",
   "author": "Johnny Hausman",
   "license": "MIT",
   "dependencies": {
      "@digiserve/ab-utils": "github:CruGlobal/ab-utils#1.10.2",
      "cote": "^1.2.0",
      "lodash": "^4.17.21",
      "redis": "^5.10.0",
      "semver": "^7.7.3"
   },
   "scripts": {
      "test": "run-p lint",
      "lint": "eslint . --fix --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  Your .js files look good.'",
      "dev": "node_modules/.bin/nodemon --inspect=0.0.0.0:9229 app.js",
      "devBuild": "run-script-os",
      "devInstall": "run-script-os",
      "devBuild:win32": "docker run -it --mount type=bind,source=%cd%,target=/app -w /app node npm install",
      "devInstall:win32": "docker run -it --mount type=bind,source=%cd%,target=/app -w /app node bash",
      "devBuild:darwin:linux": "docker run -it --mount type=bind,source=\"$(pwd)\",target=/app -w /app node npm install",
      "devInstall:darwin:linux": "docker run -it --mount type=bind,source=\"$(pwd)\",target=/app -w /app node bash"
   },
   "devDependencies": {
      "@eslint/js": "^9.15.0",
      "chai": "^4.3.7",
      "eslint": "^9.26.0",
      "eslint-config-prettier": "^10.1.8",
      "eslint-plugin-prettier": "^5.4.0",
      "mocha": "^10.4.0",
      "nodemon": "^3.1.11",
      "npm-run-all": "^4.1.5",
      "prettier": "^3.0.0",
      "run-script-os": "^1.1.5"
   },
   "overrides": {
      "@opentelemetry/api": "^1.7.0",
